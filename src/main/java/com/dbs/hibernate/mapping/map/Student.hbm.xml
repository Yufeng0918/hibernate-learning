<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.dbs.hibernate.mapping.map.Student" table="student">

        <id name="id" column="id" type="string">
            <generator class="uuid"></generator>
        </id>

        <property name="cardId" column="card_id" type="string"></property>
        <property name="name" column="name" type="string"></property>
        <property name="age" column="age" type="int"></property>

        <many-to-one name="team" column="team_id" class="com.dbs.hibernate.mapping.map.Team" cascade="none"
                     fetch="join">
        </many-to-one>

    </class>

</hibernate-mapping>
